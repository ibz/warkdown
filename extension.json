{
	"name": "Warkdown",
	"author": [
		"Ionuț Bizău"
	],
	"url": "https://github.com/ibz/warkdown/",
	"descriptionmsg": "A markdown extension for MediaWiki",
	"license-name": "GPL-2.0-or-later",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.30.0"
	},
	"AutoloadClasses": {
		"Warkdown": "includes/Warkdown.php",
		"Parsedown": "lib/Parsedown/Parsedown.php"
	},
	"config": {
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Warkdown::init"
		]
	},
	"MessagesDirs": {
		"Warkdown": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Warkdown/resources"
	},
	"ResourceModules": {
		"ext.warkdown": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"manifest_version": 2
}
